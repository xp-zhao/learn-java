<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans.xsd">

  <bean id="user" class="com.xp.spring.ioc.overview.domain.User">
    <property name="id" value="1"/>
    <property name="name" value="xp"/>
  </bean>

  <bean id="superUser" class="com.xp.spring.ioc.overview.domain.SupperUser" parent="user"
    primary="true">
    <property name="address" value="成都"/>
  </bean>

  <bean id="objectFactory"
    class="org.springframework.beans.factory.config.ObjectFactoryCreatingFactoryBean">
    <property name="targetBeanName" value="user"/>
  </bean>
</beans>
